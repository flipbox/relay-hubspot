{
  "name": "flipboxdigital/relay-hubspot",
  "description": "HubSpot RESTful Middleware ",
  "version": "2.0.0",
  "keywords": [
    "flipbox",
    "hubspot"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Flipbox Factory",
      "email": "hello@flipboxfactory.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "flipboxdigital/relay-simplecache": "dev-feature/php8",
    "guzzlehttp/guzzle":"^6.5|^7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Flipbox\\Relay\\HubSpot\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Flipbox\\Relay\\HubSpot\\Tests\\": "tests/src/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packagist.org"
    }
  ]
}